<%# Creates a new team 
Adds roles to that team %>

<%= form_for @team do |f| %> 
<%= f.label :name %><br>
<%= f.text_field :name %><br> <%#params[:team][:name] %>

<br>
<%= f.label :description %><br>
<%= f.text_field :description %><br><%#params[:team][:description]%>
<br>
Select from the roles below: <br>
<%= f.fields_for :roles do |role| %>
<%= role.collection_check_boxes :role_id, Role.all, :id, :title %><br> <%#params[:team][:role_id][:id]%>
<br>
Or add a new role: <br>
<%= role.label :title %><br>
<%= role.text_field :title %><br><%#params[:team][:roles_attributes][:title]%>
<br> 
<%= role.label :description %><br>
<%= role.text_field :description %><br><%#params[:team][:roles_attributes][:description]%>
<br> 
<%= role.label :arrival_time %><br>
<%= role.text_field :arrival_time %><br><%#params[:team][:roles_attributes][:arrival_time]%>
<% end %>

<br>
<%= f.submit "Add Team" %> <%#ideally creates a new team, creates new roles with a team id that matches new team%>
<%end %>